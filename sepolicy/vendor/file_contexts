/mnt/vendor/persist/subsys(/.*)?                u:object_r:persist_subsys_file:s0

# IR
/vendor/bin/hw/android\.hardware\.ir@1\.0-service\.xiaomi                                               u:object_r:hal_ir_default_exec:s0

