# Battery
/vendor/bin/batteryd		u:object_r:batteryd_exec:s0
/vendor/bin/batterysecret		u:object_r:batterysecret_exec:s0
/mnt/vendor/persist/subsys(/.*)?                u:object_r:persist_subsys_file:s0

# Fingerprint
/vendor/bin/hw/android\.hardware\.fingerprint@2\.1-service\.xiaomi_sm6250                               u:object_r:hal_fingerprint_default_exec:s0

# Power HAL
/vendor/bin/hw/android\.hardware\.power@1\.3-service\.xiaomi_sm6250                                     u:object_r:hal_power_default_exec:s0

# FPS Info
/sys/class/drm/sde-crtc-0/measured_fps                       u:object_r:sysfs_fps:s0

# KCAL
/sys/devices/platform/kcal_ctrl.0(/.*)?                      u:object_r:kcal_dev:s0
/sys/bus/platform/drivers/kcal_ctrl(/.*)?                    u:object_r:kcal_dev:s0

# Smart Charging
/sys/class/power_supply/battery/charging_enabled                    u:object_r:sysfs_scharge:s0

# Touchboost
/sys/module/msm_performance/parameters/touchboost		            u:object_r:sysfs_touchboost:s0
